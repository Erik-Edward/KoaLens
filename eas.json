{
  "cli": {
    "version": ">= 15.0.12",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "env": {
        "ENVIRONMENT": "development",
        "API_URL": "https://api-dev.koalens.com",
        "SENTRY_DISABLE": "true"
      }
    },
    "development-local": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug"
      },
      "env": {
        "ENVIRONMENT": "development",
        "API_URL": "https://api-dev.koalens.com",
        "SENTRY_DISABLE": "true"
      }
    },
  "preview": {
    "distribution": "internal",
    "android": {
      "buildType": "apk",
      "gradleCommand": ":app:assembleRelease"
    },
    "env": {
      "ENVIRONMENT": "preview",
      "API_URL": "https://koalens-backend.fly.dev",
      "SENTRY_DISABLE": "true"
    }
    },
    "production": {
      "autoIncrement": true,
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "ENVIRONMENT": "production",
        "API_URL": "https://api.koalens.com"
      }
    }
  },
  "submit": {
    "production": {
      "android": {
        "track": "internal"
      }
    }
  }
}